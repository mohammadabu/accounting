<odoo>
    <data>
        <record id="custom_custody_employee" model="ir.ui.view">
            <field name="name">custom custody employee</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook/page[@name='personal_information']" position="after">
                    <page name="custody_information" string="Custody Information" groups="hr.group_hr_user">
                        <group>
                            <group string="Custody Information">
                                <field name="employee_custody"/>
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>